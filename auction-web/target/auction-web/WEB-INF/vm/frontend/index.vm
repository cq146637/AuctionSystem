<script>
    window.setInterval(function () {
        if ($("#is_change_day").text() == "0") {
            return;
        }
        if($("#is_change_second").text() != "0") {
            $("#is_change_second").text(parseInt($("#is_change_second").text() - 1))
        } else {
            $("#is_change_second").text(59);
            if($("#is_change_minute").text() != "0") {
                $("#is_change_minute").text(parseInt($("#is_change_minute").text() - 1))
            } else {
                $("#is_change_minute").text(59);
                if($("#is_change_hour").text() != "0") {
                    $("#is_change_hour").text(parseInt($("#is_change_hour").text() - 1))
                } else {
                    $("#is_change_hour").text(23);
                    if($("#is_change_day").text() != "0") {
                        $("#is_change_day").text(parseInt($("#is_change_day").text() - 1))
                    } else {
                        $("#is_change_day").text('0')
                    }
                }
            }
        }
    }, 1000);
</script>